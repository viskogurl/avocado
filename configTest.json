{
  "middleware": {
    "koa-json": {
      "priority": 10,
      "module": {
        "name": "koa-json"
      }
    },
    "koa-body": {
      "opts": {
        "multipart": true
      },
      "priority": 11,
      "module": {
        "name": "koa-body"
      }
    },
    "koa-helmet": {
      "opts": {
        "contentSecurityPolicy": false
      },
      "priority": 1,
      "module": {
        "name": "koa-helmet"
      }
    },
    "router": {
      "noRequire": true,
      "opts": {
        "contentSecurityPolicy": false
      },
      "priority": 1,
      "module": {
        "name": "router",
        "methods": [
          "allowedMethods",
          "routes"
        ]
      }
    }
  }
}